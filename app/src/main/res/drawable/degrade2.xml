<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_height="150dp"
        android:layout_width="150dp">


    <item
            android:left="0dp"
            android:right="0dp"
            android:top="0dp"
            android:bottom="0dp">
