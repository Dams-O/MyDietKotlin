<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/app_bar_main"
        tools:context=".MainActivity">

    <Button
            android:text="@string/title_activity_connexion"
            android:layout_width="154dp"
            android:layout_height="80dp"
            android:id="@+id/button2"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="36dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="440dp" android:textColor="@color/colorText"
            android:background="@color/colorButton"/>
    <Button
            android:text="@string/button_inscription"
            android:layout_width="154dp"
            android:layout_height="80dp"
            android:id="@+id/button3"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="220dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="440dp"
            android:textColor="@color/colorText" android:background="@color/colorButton"/>
    <ImageView
            android:layout_width="323dp"
            android:layout_height="256dp"
            android:id="@+id/imageView3" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="92dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="44dp" app:srcCompat="@drawable/logo_3"/>
</androidx.constraintlayout.widget.ConstraintLayout>