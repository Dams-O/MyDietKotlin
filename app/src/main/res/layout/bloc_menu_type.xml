                app:layout_constraintStart_toStartOf="@+id/huile"
                app:layout_constraintTop_toTopOf="@+id/huile"
                app:layout_constraintVertical_bias="1.0" />

        <TextView
                android:id="@+id/chiffre_chocolat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_bold"
                android:text="4"
                android:textSize="25sp"
                app:layout_constraintBottom_toBottomOf="@+id/chocolat"
                app:layout_constraintEnd_toEndOf="@+id/chocolat"
                app:layout_constraintHorizontal_bias="0.515"
                app:layout_constraintStart_toStartOf="@+id/chocolat"
                app:layout_constraintTop_toTopOf="@+id/chocolat"
                app:layout_constraintVertical_bias="1.0" />

        <TextView
                android:id="@+id/chiffre_ble"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_bold"
                android:text="4"
                android:textSize="25sp"
                app:layout_constraintBottom_toBottomOf="@+id/ble"
                app:layout_constraintEnd_toEndOf="@+id/ble"
                app:layout_constraintHorizontal_bias="0.505"
                app:layout_constraintStart_toStartOf="@+id/ble"
                app:layout_constraintTop_toTopOf="@+id/ble"
                app:layout_constraintVertical_bias="1.0" />

        <TextView
                android:id="@+id/chiffre_pomme"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_bold"
                android:text="4"
                android:textSize="25sp"
                app:layout_constraintBottom_toBottomOf="@+id/pomme"
                app:layout_constraintEnd_toEndOf="@+id/pomme"
                app:layout_constraintHorizontal_bias="0.515"
                app:layout_constraintStart_toStartOf="@+id/pomme"
                app:layout_constraintTop_toTopOf="@+id/pomme"
                app:layout_constraintVertical_bias="1.0" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</RelativeLayout>