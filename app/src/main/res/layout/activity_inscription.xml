<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Inscription">
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="14dp"
            android:id="@+id/textView2" tools:ignore="MissingConstraints"
            android:text="@string/identifiant"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="36dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="88dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="14dp"
            android:id="@+id/textView3" tools:ignore="MissingConstraints"
            android:text="@string/password"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="36dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="196dp"/>
    <EditText
            android:id="@+id/message2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType=
                    "textCapSentences|textAutoCorrect" tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="36dp" app:layout_constraintTop_toBottomOf="@+id/textView2"
            android:layout_marginTop="16dp" android:autofillHints="" tools:targetApi="o"/>
    <EditText
            android:id="@+id/message3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword" tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="36dp" app:layout_constraintTop_toBottomOf="@+id/textView2"
            android:layout_marginTop="128dp" android:autofillHints="" tools:targetApi="o"/>
    <Button
            android:text="@string/button_inscription"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button" android:layout_marginTop="176dp"
            app:layout_constraintTop_toBottomOf="@+id/message2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="28dp"/>
</android.support.constraint.ConstraintLayout>